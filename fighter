	local player = game.Players.LocalPlayer
	local whitelisted = {
		[10224205063] = true,
		[10192581807] = true
	}	 
	
	if not whitelisted then
		player:Kick("Not Whitelisted Dm @linearLt on discord")
	end
end
coroutine.wrap(RWINQ_fake_script)()
