coroutine.wrap(UESQYP_fake_script)()
local function RTTMXC_fake_script()  AntiFling.LocalScript 
	local script = Instance.new('LocalScript', AntiFling)

	
	
	
	local Services = setmetatable({}, {__index = function(Self, Index)
		local NewService = game.GetService(game, Index)
		if NewService then
			Self[Index] = NewService
		end
		return NewService
	end})
	
	
	local LocalPlayer = Services.Players.LocalPlayer
	
	
	local antifling = false
	local Toggle = false
	local Parent = script.Parent.Parent
	
	local function PlayerAdded(Player)
		local Detected = false
		local Character;
		local PrimaryPart;
	
		local function CharacterAdded(NewCharacter)
			Character = NewCharacter
			repeat
				wait()
				PrimaryPart = NewCharacter:FindFirstChild("HumanoidRootPart")
			until PrimaryPart
			Detected = false
		end
	
		CharacterAdded(Player.Character or Player.CharacterAdded:Wait())
		Player.CharacterAdded:Connect(CharacterAdded)
		Services.RunService.RenderStepped:Connect(function()
			if Toggle and (Character and Character:IsDescendantOf(workspace)) and (PrimaryPart and PrimaryPart:IsDescendantOf(Character)) then
				if PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 or PrimaryPart.AssemblyLinearVelocity.Magnitude > 100 then
					Detected = true
					for i,v in ipairs(Character:GetDescendants()) do
						if v:IsA("BasePart") then
							v.CanCollide = false
							v.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
							v.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
							v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
						end
					end
	
					PrimaryPart.CanCollide = false
					PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
					PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
					PrimaryPart.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
				end
			end
		end)
	end
	
	for i,v in ipairs(Services.Players:GetPlayers()) do
		if v ~= LocalPlayer then
			PlayerAdded(v)
		end
	end
	Services.Players.PlayerAdded:Connect(PlayerAdded)
	
	local LastPosition = nil
	Services.RunService.RenderStepped:Connect(function()
		pcall(function()
			local PrimaryPart = LocalPlayer.Character.PrimaryPart
			if Toggle and (PrimaryPart.AssemblyLinearVelocity.Magnitude > 250 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 250) then
				PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
				PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
				PrimaryPart.CFrame = LastPosition
			elseif PrimaryPart.AssemblyLinearVelocity.Magnitude < 50 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 then
				LastPosition = PrimaryPart.CFrame
			end
		end)
	end)
	
	script.Parent.MouseButton1Click:connect(function()
		if not Toggle then
			Toggle = true
			if Toggle == true then 
				antifling = true
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		elseif Toggle then
			Toggle = false
			if Toggle == false then
				antifling = false
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
	
	
end
coroutine.wrap(RTTMXC_fake_script)()
local function SSKU_fake_script()  NoCollusion.NoCollisionToggle 
	local script = Instance.new('LocalScript', NoCollusion)

	local player = game:GetService("Players").LocalPlayer
	local Toggle = false
	
	script.Parent.MouseButton1Click:Connect(function()
	    Toggle = not Toggle
	
	    if Toggle then
	        script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	        script.Parent.BorderColor3 = Color3.fromRGB(74, 74, 74)  Change BorderPixelColor
	        local character = player.Character or player.CharacterAdded:Wait()
	        for _, part in character:GetChildren() do
	            if part:IsA("BasePart") then
	                part.CanCollide = false
	            end
	        end
	    else
	        script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	        script.Parent.BorderColor3 = Color3.fromRGB(15, 15, 15)  Change BorderPixelColor
	        local character = player.Character or player.CharacterAdded:Wait()
	        for _, part in character:GetChildren() do
	            if part:IsA("BasePart") then
	                part.CanCollide = true
	            end
	        end
	    end
	end)
	
	
end
coroutine.wrap(SSKU_fake_script)()
local function IKWY_fake_script()  AntiFling_2.NoCollisionToggle 
	local script = Instance.new('LocalScript', AntiFling_2)

	
	
	local Players = game:GetService("Players")
	local lp = Players.LocalPlayer
	local Toggle = false
	local Parent = script.Parent.Parent
	local antiFallEnabled = false
	
	local function onCharacterAdded(character)
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.StateChanged:Connect(function(oldState, newState)
				if antiFallEnabled and newState == Enum.HumanoidStateType.FallingDown then
					humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
				end
			end)
		end
	end
	
	lp.CharacterAdded:Connect(onCharacterAdded)
	
	if lp.Character then
		onCharacterAdded(lp.Character)
	end
	
	script.Parent.MouseButton1Click:connect(function()
		if not Toggle then
			Toggle = true
			if Toggle == true then 
				antiFallEnabled = true
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		elseif Toggle then
			Toggle = false
			if Toggle == false then
				antiFallEnabled = false
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
	
	
end
coroutine.wrap(IKWY_fake_script)()
local function MHNTZ_fake_script()  AntiVoid.NoCollisionToggle 
	local script = Instance.new('LocalScript', AntiVoid)

	local Players = game:GetService("Players")
	local lp = Players.LocalPlayer
	local Toggle = false
	local Parent = script.Parent.Parent
	local workspace = game:GetService("Workspace")
	
	script.Parent.MouseButton1Click:Connect(function()
		if not Toggle then
			Toggle = true
			if Toggle == true then 
				workspace.FallenPartsDestroyHeight = 0/0
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		elseif Toggle then
			Toggle = false
			if Toggle == false then
				workspace.FallenPartsDestroyHeight = -500
			end
			script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
	
	
end
