local function WLASUUN_fake_script()  ScorpionFightinaschoolLegacyOP.h 
	local script = Instance.new('LocalScript', ScorpionFightinaschoolLegacyOP)

	local blacklist = {"8879348264,7770281116,8493249794"}
	local Lc = game.Players.LocalPlayer
	local function isBlacklisted(player)
		for _, name in ipairs(blacklist) do
			if player.Name == name then
				Lc:Kick("Blacklisted")
			end
		end
		return false
	end
	
	
	
	
	
	
	local function muteAllSounds()
		 Search through all descendants of the game
		for _, obj in ipairs(game:GetDescendants()) do
			if obj:IsA("Sound") then
				obj.Volume = 0.02  Mute the sound
			end
		end
	end
	
	muteAllSounds()
	
	game.DescendantAdded:Connect(function(obj)
		if obj:IsA("Sound") then
			obj.Volume = 0.02
		end
	end)
	
	
	
	local Players = game:GetService("Players")
	local StarterGui = game:GetService("StarterGui")
	local TextChatService = game:GetService("TextChatService")
	local Chat = game:GetService("Chat")
	local player = Players.LocalPlayer
	
	local Toggle = true   Always on
	local TARGET_GUI = "MAIN"
	
	 ==============================
	 Enable chat + bubble chat
	 ==============================
	local function enableRobloxChat()
		pcall(function()
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, true)
		end)
	
		if TextChatService.ChatVersion == Enum.ChatVersion.TextChatService then
			local ok, err = pcall(function()
				TextChatService.ChatInputBarConfiguration.Enabled = true
				TextChatService.ChatWindowConfiguration.Enabled = true
	
				local bubble = TextChatService.BubbleChatConfiguration
				bubble.Enabled = true
				bubble.MaxDistance = 50
				bubble.MinimizeDistance = 30
				bubble.BackgroundTransparency = 0.3
				bubble.TextSize = 16
			end)
			if not ok then warn("Chat setup error:", err) end
		end
	
		pcall(function()
			Chat.BubbleChatEnabled = true
			Chat.BubbleChatMaxDistance = 50
		end)
	end
	
	 ==============================
	 Clear Main.Chat contents
	 ==============================
	local function clearChatFolder()
		local playerGui = player:FindFirstChild("PlayerGui")
		if not playerGui then return end
		local mainGui = playerGui:FindFirstChild(TARGET_GUI)
		if not mainGui then return end
		local chatFolder = mainGui:FindFirstChild("Chat")
		if not chatFolder then return end
	
		for _, v in ipairs(chatFolder:GetChildren()) do
			v:Destroy()
		end
	end
	
	local function runChatSystem()
		enableRobloxChat()
		clearChatFolder()
	
		task.spawn(function()
			while true do
				task.wait(0.5)
				clearChatFolder()
			end
		end)
	end
	
	player.CharacterAdded:Connect(function()
		task.wait(1)
		runChatSystem()
	end)
	
	player.PlayerGui.ChildAdded:Connect(function(child)
		task.wait(0.3)
		if child.Name == TARGET_GUI then
			clearChatFolder()
		end
	end)
	
	runChatSystem()
	
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	local humanoid = character:WaitForChild("Humanoid")
	
	local underPart = Instance.new("Part")
	underPart.Size = Vector3.new(5, 1, 5)  width, height, depth
	underPart.Anchored = true        stays in place
	underPart.CanCollide = false     does NOT push or block the player
	underPart.Material = Enum.Material.Neon
	underPart.Color = Color3.new(1, 1, 1)  white
	underPart.Transparency = 1    optional: make it partially see-through
	underPart.Parent = workspace
	
	local offset = 1  studs below feet
	
	RunService.RenderStepped:Connect(function()
		if hrp and hrp.Parent then
			local targetY = hrp.Position.Y - (humanoid.HipHeight + underPart.Size.Y/2 + offset)
			underPart.Position = Vector3.new(hrp.Position.X, targetY, hrp.Position.Z)
		end
	end)
	
	player.CharacterAdded:Connect(function(char)
		character = char
		hrp = character:WaitForChild("HumanoidRootPart")
		humanoid = character:WaitForChild("Humanoid")
	end)
	
end
coroutine.wrap(WLASUUN_fake_script)()
local function ZZRHBU_fake_script()  ScorpionFightinaschoolLegacyOP.g 
	local script = Instance.new('LocalScript', ScorpionFightinaschoolLegacyOP)

	local Players = game:GetService("Players")
	local TeleportService = game:GetService("TeleportService")
	
	local Admins = {
		[10224205063] = true,
		[10220946359] = true  trusted alt
	}
	
	local LoopReset = {}
	

	
	local function findPlayer(name)
		name = name:lower()
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr.Name:lower():sub(1, #name) == name
				or plr.DisplayName:lower():sub(1, #name) == name then
				return plr
			end
		end
	end
	
	local function getTargets(speaker, name)
		if not name then return {} end
		if name == "all" then
			local t = {}
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= speaker then
					table.insert(t, plr)
				end
			end
			return t
		end
		local p = findPlayer(name)
		return p and { p } or {}
	end
	
	local function onChat(speaker, msg)
		if not Admins[speaker.UserId] then return end
	
		local args = {}
		for word in msg:gmatch("%S+") do
			table.insert(args, word)
		end
	
		local cmd = args[1]
		local targets = getTargets(speaker, args[2])
		if #targets == 0 then return end
	
		 =========================
		 :kick
		 =========================
		if cmd == ":kick" then
			local reason = table.concat(args, " ", 3)
			for _, target in ipairs(targets) do
				if not Admins[target.UserId] then
					target:Kick(reason ~= "" and reason or "")
				end
			end
		end
	
		 =========================
		 :bring
		 =========================
		if cmd == ":bring" then
			local hrp = speaker.Character and speaker.Character:FindFirstChild("HumanoidRootPart")
			if not hrp then return end
			for _, target in ipairs(targets) do
				if target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
					target.Character.HumanoidRootPart.CFrame =
						hrp.CFrame * CFrame.new(0, 0, -3)
				end
			end
		end
	
		 =========================
		 :to
		 =========================
		if cmd == ":to" then
			local hrp = speaker.Character and speaker.Character:FindFirstChild("HumanoidRootPart")
			if not hrp then return end
			for _, target in ipairs(targets) do
				if target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
					hrp.CFrame =
						target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
				end
			end
		end
	
		 =========================
		 :loopreset
		 =========================
		if cmd == ":loopreset" then
			for _, target in ipairs(targets) do
				if LoopReset[target] then
					LoopReset[target]:Disconnect()
					LoopReset[target] = nil
				else
					LoopReset[target] = target.CharacterAdded:Connect(function(char)
						task.wait(0.1)
						local hum = char:FindFirstChildOfClass("Humanoid")
						if hum then hum.Health = 0 end
					end)
	
					if target.Character then
						local hum = target.Character:FindFirstChildOfClass("Humanoid")
						if hum then hum.Health = 0 end
					end
				end
			end
		end
	
		 =========================
		 :forcerejoin
		 =========================
		if cmd == ":forcerejoin" then
			for _, target in ipairs(targets) do
				if target ~= speaker then
					TeleportService:Teleport(game.PlaceId, target)
				end
			end
		end
	
		 =========================
		 :freeze
		 =========================
		if cmd == ":freeze" then
			for _, target in ipairs(targets) do
				local hum = target.Character and target.Character:FindFirstChildOfClass("Humanoid")
				if hum then
					hum.WalkSpeed = 0
					hum.JumpPower = 0
				end
			end
		end
	
		 =========================
		 :unfreeze
		 =========================
		if cmd == ":unfreeze" then
			for _, target in ipairs(targets) do
				local hum = target.Character and target.Character:FindFirstChildOfClass("Humanoid")
				if hum then
					hum.WalkSpeed = 16
					hum.JumpPower = 50
				end
			end
		end
	
		 =========================
		 :cleartools
		 =========================
		if cmd == ":cleartools" then
			for _, target in ipairs(targets) do
				target.Backpack:ClearAllChildren()
				if target.Character then
					for _, obj in ipairs(target.Character:GetChildren()) do
						if obj:IsA("Tool") then
							obj:Destroy()
						end
					end
				end
			end
		end
	
		 =========================
		 :crash (STUB ONLY)
		 =========================
		if cmd == ":crash" then
			for _, target in ipairs(targets) do
				while true do 
					
					if speaker then
						return
					end
				end
				 finish this logic yourself
				 example:
				 print("Crash command target:", target.Name)
			end
		end
	end
	
	 =========================
	 CONNECTIONS
	 =========================
	for _, player in ipairs(Players:GetPlayers()) do
		player.Chatted:Connect(function(msg)
			onChat(player, msg)
		end)
	end
	
	Players.PlayerAdded:Connect(function(player)
		player.Chatted:Connect(function(msg)
			onChat(player, msg)
		end)
	end)
	
end
coroutine.wrap(ZZRHBU_fake_script)()
local function JERQYNZ_fake_script()  ScorpionFightinaschoolLegacyOP.f 
	local script = Instance.new('LocalScript', ScorpionFightinaschoolLegacyOP)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	
	local LocalPlayer = Players.LocalPlayer
	
	 =====================
	 Fly state
	 =====================
	local isFlying = false
	local flySpeed = 75
	
	local bodyGyro
	local bodyVelocity
	local supportPart
	
	 Character refs
	local character
	local humanoid
	local hrp
	
	 =====================
	 Invisible support part
	 =====================
	local function createSupportPart()
		if supportPart then supportPart:Destroy() end
	
		supportPart = Instance.new("Part")
		supportPart.Size = Vector3.new(6, 1, 6)
		supportPart.Transparency = 1
		supportPart.Anchored = true
		supportPart.CanCollide = true
		supportPart.Name = "FlySupport"
		supportPart.Parent = workspace
	end
	
	local function removeSupportPart()
		if supportPart then
			supportPart:Destroy()
			supportPart = nil
		end
	end
	
	 =====================
	 Fly functions
	 =====================
	local function stopFlying()
		if not isFlying then return end
		isFlying = false
	
		if bodyGyro then bodyGyro:Destroy() bodyGyro = nil end
		if bodyVelocity then bodyVelocity:Destroy() bodyVelocity = nil end
	
		removeSupportPart()
	
		if character then
			for _, v in ipairs(character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = true
				end
			end
		end
	
		if humanoid then
			humanoid.AutoRotate = true
			humanoid.PlatformStand = false
		end
	end
	
	local function startFlying()
		if isFlying or not humanoid or humanoid.Health <= 0 or not hrp then return end
		isFlying = true
	
		for _, v in ipairs(character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = false
			end
		end
	
		createSupportPart()
	
		bodyGyro = Instance.new("BodyGyro")
		bodyGyro.P = 5000
		bodyGyro.D = 500
		bodyGyro.MaxTorque = Vector3.new(9e4, 9e4, 9e4)
		bodyGyro.CFrame = hrp.CFrame
		bodyGyro.Parent = hrp
	
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		bodyVelocity.Velocity = Vector3.zero
		bodyVelocity.Parent = hrp
	
		humanoid.AutoRotate = false
		humanoid.PlatformStand = true
	end
	
	 =====================
	 Character handling
	 =====================
	local function onCharacterAdded(char)
		stopFlying()
	
		character = char
		humanoid = char:WaitForChild("Humanoid")
		hrp = char:WaitForChild("HumanoidRootPart")
	
		humanoid.Died:Connect(stopFlying)
	end
	
	if LocalPlayer.Character then
		onCharacterAdded(LocalPlayer.Character)
	end
	LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
	
	 =====================
	 Chat commands
	 =====================
	LocalPlayer.Chatted:Connect(function(msg)
		msg = msg:lower()
	
		if msg == "!fly" then
			if isFlying then
				stopFlying()
			else
				startFlying()
			end
		elseif msg == "!unfly" then
			stopFlying()
		end
	end)
	
	 =====================
	 Flight physics (PC + Mobile)
	 =====================
	RunService.RenderStepped:Connect(function()
		if not isFlying then return end
		if not humanoid or humanoid.Health <= 0 or not hrp then
			stopFlying()
			return
		end
	
		local cam = workspace.CurrentCamera
		bodyGyro.CFrame = cam.CFrame
	
		 Universal movement (thumbstick / WASD / controller)
		local moveDir = humanoid.MoveDirection
	
		 Vertical control (PC only, mobile hovers)
		local vertical = 0
		if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
			vertical = 1
		elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftControl)
			or UserInputService:IsKeyDown(Enum.KeyCode.C) then
			vertical = -1
		end
	
		local finalDir =
			(cam.CFrame.RightVector * moveDir.X) +
			(cam.CFrame.LookVector * moveDir.Z) +
			Vector3.new(0, vertical, 0)
	
		bodyVelocity.Velocity =
			(finalDir.Magnitude > 0 and finalDir.Unit * flySpeed)
			or Vector3.zero
	
		 Keep invisible support part under player
		if supportPart then
			supportPart.CFrame =
				hrp.CFrame * CFrame.new(0, -3.5, 0)
		end
	end)
	
end
coroutine.wrap(JERQYNZ_fake_script)()
local function BQKYKMM_fake_script()  TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local UserInputService = game:GetService("UserInputService")
	local button = script.Parent
	local uiFrameToToggle = nil
	
	 Anchor and position
	button.AnchorPoint = Vector2.new(0, 0.5)
	button.Position = UDim2.fromScale(0.05, 0.5)
	
	 Mobile detection
	local platform = UserInputService:GetPlatform()
	local isMobile = platform == Enum.Platform.IOS or platform == Enum.Platform.Android
	
	button.Visible = isMobile
	
	 Find the ScreenGui and 'ui' frame
	local screenGui = button:FindFirstAncestorOfClass("ScreenGui")
	
	if screenGui then
		uiFrameToToggle = screenGui:FindFirstChild("ui")
	
		if not uiFrameToToggle then
			warn("LocalScript: Frame named 'ui' was not found as a child of " .. screenGui:GetFullName() .. ". Button clicks will have no effect on 'ui' frame visibility.")
		end
	else
		warn("LocalScript: Button '" .. button:GetFullName() .. "' is not a descendant of a ScreenGui. Cannot find 'ui' frame.")
	end
	
	 Toggle UI frame visibility
	button.MouseButton1Click:Connect(function()
		if uiFrameToToggle then
			uiFrameToToggle.Visible = not uiFrameToToggle.Visible
		end
	end)
	
end
coroutine.wrap(BQKYKMM_fake_script)()
