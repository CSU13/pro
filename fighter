local function LBHGYE_fake_script()  AutoEquipFist.LocalScript 
	local script = Instance.new('LocalScript', AutoEquipFist)

	local Players = game:GetService("Players")
	local localPlayer = Players.LocalPlayer
	local toolName = "Fight"
	
	local EquipToggle = false
	local UserInputService = game:GetService("UserInputService")
	
	 Equip tool function
	local function equipTool()
		local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
		if not character then return end
	
		local backpack = localPlayer:WaitForChild("Backpack")
		local toolInstanceInBackpack = backpack:FindFirstChild(toolName)
	
		if toolInstanceInBackpack then
			if not character:FindFirstChild(toolName) then
				toolInstanceInBackpack.Parent = character
			end
		end
	end
	
	 Unequip tool function
	local function unequipTool()
		local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
		if not character then return end
	
		local backpack = localPlayer:WaitForChild("Backpack")
		local toolInstanceInCharacter = character:FindFirstChild(toolName)
	
		if toolInstanceInCharacter then
			toolInstanceInCharacter.Parent = backpack
		end
	end
	
	 Function to toggle the auto-equip state
	local function toggleAutoEquip()
		if not EquipToggle then
			EquipToggle = true
			button.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
			print("Auto Equip ON!")
		else
			EquipToggle = false
			button.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
			print("Auto Equip OFF!")
			unequipTool()
		end
	
		 Unlock cursor logic
		UserInputService.MouseIconEnabled = true
		game:GetService("CoreGui"):SetCore("TopbarEnabled", true)
		localPlayer.MouseLock = false
	end
	
	 Toggle Button Logic
	local button = script.Parent
	button.MouseButton1Click:Connect(toggleAutoEquip)
	
	 Keybind: Toggle auto-equip when "T" is pressed
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end  Ignore if the player is typing in chat or GUI
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.T then
			toggleAutoEquip()
		end
	end)
	
	 Main loop to constantly check for equip action
	while true do
		if EquipToggle then
			equipTool()
		end
		task.wait(0.3)
	end
	
end
coroutine.wrap(LBHGYE_fake_script)()
local function OJMFKMO_fake_script()  JoinLegacy.LocalScript 
	local script = Instance.new('LocalScript', JoinLegacy)

	local TeleportService = game:GetService("TeleportService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	
	local targetPlaceId = 98335050422796 
	
	script.Parent.MouseButton1Click:Connect(function()
	    if targetPlaceId == 0 then
	        warn("Target Place ID is not set. Please edit the script and set the 'targetPlaceId' variable.")
	        return
	    end
	
	    local success, errorMessage = pcall(function()
	        TeleportService:Teleport(targetPlaceId, LocalPlayer)
	    end)
	
	    if not success then
	        warn("Teleport failed: " .. errorMessage)
	    else
	        print("Teleporting to Place ID: " .. targetPlaceId)
	    end
	end)
	
	
end
coroutine.wrap(OJMFKMO_fake_script)()
local function BCNGKWC_fake_script()  ui.afsf 
	local script = Instance.new('LocalScript', ui)

	local frame = script.Parent
	local UIS = game:GetService("UserInputService")
	
	local dragging = false
	local dragStartPos
	local startPos
	
	 Universal drag (PC + Mobile)
	local function updatePosition(input)
		local delta = input.Position - dragStartPos
		frame.Position = UDim2.new(
			startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y
		)
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 
			or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStartPos = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement 
			or input.UserInputType == Enum.UserInputType.Touch) then
			updatePosition(input)
		end
	end)
	
end
coroutine.wrap(BCNGKWC_fake_script)()
local function PGQX_fake_script()  TeleportToAll.LocalScript 
	local script = Instance.new('LocalScript', TeleportToAll)

	local plrs = game:GetService("Players")
	local plr = plrs.LocalPlayer
	local run_service = game:GetService("RunService")
	
	local target: string
	local teleportEnabled = false
	local toggleButton = script.Parent
	local delayTextBox = toggleButton:FindFirstChild("TextBox")  Get the TextBox
	local ignoreFriendsButton = toggleButton:FindFirstChild("IgnoreFriendsButton")  Get the IgnoreFriendsButton
	
	local FLUNG_VELOCITY_THRESHOLD = 1000
	local PREDICTION_TIME = 0  Time in seconds to predict ahead
	
	local onColor = Color3.fromRGB(74, 74, 74)
	local offColor = Color3.fromRGB(15, 15, 15)
	
	
	local teleportDelay = 1  Default delay of 1 second
	local lastTeleportTimestamp = 0
	local ignoreFriendsEnabled = false
	
	toggleButton.BackgroundColor3 = offColor
	if delayTextBox then
		delayTextBox.Text = "TeleportDelay" 
	end
	
	if ignoreFriendsButton then
	end
	
	local function find_target(name: string)
		if not name or #name < 1 then
			return
		end
	
		name = string.lower(name)
	
		local plrs_table = plrs:GetPlayers()
	
		for _, player_candidate in plrs_table do
			if player_candidate and player_candidate ~= plr then  Ensure not targeting self
				local lower_name = string.lower(player_candidate.Name)
				local lower_display = string.lower(player_candidate.DisplayName)
	
				if string.sub(lower_name, 1, #name) == name or string.sub(lower_display, 1, #name) == name then
					if ignoreFriendsEnabled and plr:IsFriendsWith(player_candidate.UserId) then
						return nil  Don't target friends if ignoring
					end
					return player_candidate
				end
			end
		end
		return nil
	end
	
	local function generate_rand()
		local all_plrs = {}
		local all_player_instances = plrs:GetPlayers()
	
		for _, v in all_player_instances do
			if v and v.Character and v ~= plr then
				if ignoreFriendsEnabled and plr:IsFriendsWith(v.UserId) then
					 Skip friend
				else
					local char = v.Character
					local humanoid = char:FindFirstChildWhichIsA("Humanoid")
					local hrp = char:FindFirstChild("HumanoidRootPart")
	
					if humanoid and humanoid.Health ~= 0 and not char:FindFirstChild("KO") and hrp then
						if hrp.AssemblyLinearVelocity.Y < FLUNG_VELOCITY_THRESHOLD then
							table.insert(all_plrs, v)
						end
					end
				end
			end
		end
	
		if #all_plrs < 1 then
			return
		end
	
		local chosen_player = all_plrs[math.random(1, #all_plrs)]
		return chosen_player
	end
	
	toggleButton.MouseButton1Click:Connect(function()
		teleportEnabled = not teleportEnabled
		if teleportEnabled then
			toggleButton.BackgroundColor3 = onColor
			print("Teleportation enabled.")
		else
			toggleButton.BackgroundColor3 = offColor
			print("Teleportation disabled.")
			if delayTextBox then
				 If the current text is not a number, set to "TeleportDelay"
				 Otherwise, leave it as the number it is, reflecting the preserved delay.
				local currentDelayNum = tonumber(delayTextBox.Text)
				if not currentDelayNum then
					delayTextBox.Text = "TeleportDelay"
				end
			end
			 teleportDelay variable retains its current value (line `teleportDelay = 1` removed)
		end
	end)
	
	if ignoreFriendsButton then
		ignoreFriendsButton.MouseButton1Click:Connect(function()
			ignoreFriendsEnabled = not ignoreFriendsEnabled
			if ignoreFriendsEnabled then
			else
			end
		end)
	end
	
	if delayTextBox then
		delayTextBox.Focused:Connect(function()
		    if not teleportEnabled then
		        return  Do nothing if teleport is not enabled
		    end
		    local currentText = delayTextBox.Text
		    if currentText == "Delay" or currentText == "TeleportDelay" or tonumber(currentText) ~= nil then
		        delayTextBox.Text = ""  Clear for new input
		    end
		end)
	
		delayTextBox.FocusLost:Connect(function()
		    if not teleportEnabled then
		         If teleport is off, and focus is lost, ensure text reflects current teleportDelay or "TeleportDelay"
		        if delayTextBox then
		            local currentDelayNum = tonumber(delayTextBox.Text)
		            if not currentDelayNum then  If text became non-numeric (e.g. empty)
		                 Check if teleportDelay itself is a valid number to display
		                if teleportDelay and teleportDelay >= 0 then
		                      This case might be tricky if teleportDelay was from a non-numeric "TeleportDelay" text.
		                      For simplicity, if it's off and text is invalid, revert to "TeleportDelay"
		                     delayTextBox.Text = "TeleportDelay"
		                else
		                    delayTextBox.Text = "TeleportDelay"  Default placeholder
		                end
		            end
		        end
		        return
		    end
		    local inputText = delayTextBox.Text
		    local num = tonumber(inputText)
	
		    if num and num >= 0 then
		        teleportDelay = num
		        delayTextBox.Text = tostring(num)  Display the number
		    else
		         When input is invalid (and teleport is ON), reset to a default placeholder and delay
		        delayTextBox.Text = "Delay" 
		        teleportDelay = 1  Reset to default delay
		    end
		end)
	end
	
	run_service.Stepped:Connect(function()
		if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
			local myHRP = plr.Character.HumanoidRootPart
			local myHumanoid = plr.Character:FindFirstChildWhichIsA("Humanoid")
			if myHumanoid and myHumanoid.Health ~= 0 then
				if teleportEnabled then  Check if teleportation is enabled
					local found_tar = find_target(target)
	
					if found_tar and found_tar.Character then
						local targetChar = found_tar.Character
						local targetHumanoid = targetChar:FindFirstChildWhichIsA("Humanoid")
						local targetHRP = targetChar:FindFirstChild("HumanoidRootPart")
	
						if targetHumanoid and targetHumanoid.Health ~= 0 and targetHRP then
							if targetHRP.AssemblyLinearVelocity.Y < FLUNG_VELOCITY_THRESHOLD then
								local currentTime = os.clock()
								if currentTime - lastTeleportTimestamp >= teleportDelay then
									local targetVelocity = targetHRP.AssemblyLinearVelocity
									local predictedCFrame = targetHRP.CFrame * CFrame.new(targetVelocity * PREDICTION_TIME)
									myHRP.CFrame = predictedCFrame
									lastTeleportTimestamp = currentTime  Update the timestamp
								end
							end
						end
					end
				end
			end
		end
	end)
	
	task.spawn(function()
		pcall(function()
			while true do
				if teleportEnabled then  Only generate new targets if teleport is on
					local possible_rand = generate_rand()
					if possible_rand then
						target = possible_rand.Name
					else
						 No valid target found (e.g., all are friends and ignored)
						 target = nil  Or keep last target, or handle as needed
					end
				end
				task.wait()  Reduced frequency of random target generation to once per second
			end
		end)
	end)
	
	
end
coroutine.wrap(PGQX_fake_script)()
local function WVFKMKV_fake_script()  AutoJoinParty.LocalScript 
	local script = Instance.new('LocalScript', AutoJoinParty)

	local Players = game:GetService("Players")
	
	local localPlayer = Players.LocalPlayer
	
	local button = script.Parent
	local textBox = button:FindFirstChildWhichIsA("TextBox")
	
	if not textBox then
	    warn("Party Join script: TextBox not found under " .. button:GetFullName())
	    script.Enabled = false
	    return
	end
	
	local isToggledOn = false
	local currentPlayerInParty = nil 
	local partyRejoinTask = nil
	local REJOIN_CHECK_INTERVAL = .1
	
	local function findPlayerByName(name)
	    if not name or #name == 0 then
	        return nil
	    end
	    name = name:lower()
	    local allPlayers = Players:GetPlayers()
	    for i = 1, #allPlayers do
	        local pl = allPlayers[i]
	        if pl then
	            local playerNameLower = pl.Name:lower()
	            local displayNameLower = pl.DisplayName:lower()
	            if playerNameLower:sub(1, #name) == name or displayNameLower:sub(1, #name) == name then
	                return pl
	            end
	        end
	    end
	    return nil
	end
	
	local function firePartyRemote(eventName, targetPlayerObject)
	    if not targetPlayerObject then
	        return
	    end
	    if not targetPlayerObject.Parent then 
	        return
	    end
	
	    local communicate = localPlayer:FindFirstChild("Communicate")
	    if not communicate then
	        warn("Party script: Communicate folder not found on LocalPlayer.")
	        return
	    end
	
	    local interface = communicate:FindFirstChild("Interface")
	    if not interface then
	        warn("Party script: Interface folder not found under Communicate.")
	        return
	    end
	
	    local remoteEventInstance = interface:FindFirstChild("RemoteEvent")
	    if not remoteEventInstance or not remoteEventInstance:IsA("RemoteEvent") then
	        warn("Party script: RemoteEvent not found or not a RemoteEvent under Interface.")
	        return
	    end
	
	    local args = {
	        eventName,
	        targetPlayerObject
	    }
	
	    local success, err = pcall(function()
	        remoteEventInstance:FireServer(unpack(args))
	    end)
	
	    if not success then
	        warn("Error firing party remote (" .. eventName .. " for " .. targetPlayerObject.Name .. "): " .. tostring(err))
	    end
	end
	
	local function startPartyRejoinLoop()
	    if not currentPlayerInParty or currentPlayerInParty == localPlayer then
	        print("AutoJoinParty: Cannot start rejoin loop, invalid or self target.")
	        if partyRejoinTask then 
	            task.cancel(partyRejoinTask)
	            partyRejoinTask = nil
	        end
	        return
	    end
	
	    if partyRejoinTask then 
	        task.cancel(partyRejoinTask)
	        partyRejoinTask = nil
	    end
	
	    partyRejoinTask = task.spawn(function()
	        if not (currentPlayerInParty and currentPlayerInParty.Parent and currentPlayerInParty ~= localPlayer) then
	            print("AutoJoinParty: Rejoin loop not starting, target became invalid before task execution for " .. (currentPlayerInParty and currentPlayerInParty.Name or "unknown target"))
	            partyRejoinTask = nil
	            return
	        end
	        
	        local targetNameForLoop = currentPlayerInParty.Name 
	        print("AutoJoinParty: Rejoin loop started for " .. targetNameForLoop)
	        
	        while isToggledOn and currentPlayerInParty and currentPlayerInParty.Parent and currentPlayerInParty ~= localPlayer do
	            local freshTarget = Players:FindFirstChild(currentPlayerInParty.Name)
	            if not freshTarget or freshTarget ~= currentPlayerInParty then
	                print("AutoJoinParty (Loop): Target " .. targetNameForLoop .. " no longer valid or found in Players service. Stopping loop.")
	                break 
	            end
	
	            print("AutoJoinParty (Loop): Attempting to join/stay in party with " .. currentPlayerInParty.Name)
	            firePartyRemote("AcceptPartyInvite", currentPlayerInParty)
	            
	            local waitSuccess = pcall(task.wait, REJOIN_CHECK_INTERVAL)
	            if not waitSuccess then 
	                print("AutoJoinParty (Loop): Task wait interrupted for " .. targetNameForLoop .. ", likely cancelled.")
	                break
	            end
	        end
	        
	        print("AutoJoinParty: Rejoin loop ended for target: " .. targetNameForLoop)
	        partyRejoinTask = nil 
	    end)
	end
	
	button.MouseButton1Click:Connect(function()
	    isToggledOn = not isToggledOn
	
	    if partyRejoinTask then
	        task.cancel(partyRejoinTask)
	        partyRejoinTask = nil
	        print("AutoJoinParty: Previous rejoin loop cancelled due to button click.")
	    end
	
	    if isToggledOn then
	        button.BackgroundColor3 = Color3.fromRGB(74, 74, 74) 
	        print("AutoJoinParty: Toggled ON.")
	
	        local targetName = textBox.Text
	        local newTargetPlayer = findPlayerByName(targetName)
	
	        if currentPlayerInParty and currentPlayerInParty.Parent and currentPlayerInParty ~= newTargetPlayer and currentPlayerInParty ~= localPlayer then
	            print("AutoJoinParty: Target changed from " .. currentPlayerInParty.Name .. ". Attempting to leave their party.")
	            firePartyRemote("Leave_Party", currentPlayerInParty)
	        end
	        
	        currentPlayerInParty = newTargetPlayer 
	
	        if currentPlayerInParty and currentPlayerInParty.Parent and currentPlayerInParty ~= localPlayer then
	            print("AutoJoinParty: New target is " .. currentPlayerInParty.Name .. ". Starting rejoin attempts.")
	            startPartyRejoinLoop() 
	        elseif currentPlayerInParty == localPlayer then
	            print("AutoJoinParty: Target is self. Cannot join own party. Loop not started.")
	            currentPlayerInParty = nil 
	        else
	            print("AutoJoinParty: No valid target found or target left (" .. (targetName or "empty input") .. "). Loop not started.")
	            currentPlayerInParty = nil 
	        end
	
	    else  TOGGLING OFF
	        button.BackgroundColor3 = Color3.fromRGB(15, 15, 15) 
	        
	        if currentPlayerInParty and currentPlayerInParty.Parent and currentPlayerInParty ~= localPlayer then
	            print("AutoJoinParty (Untoggled): Attempting to leave party of " .. currentPlayerInParty.Name)
	            firePartyRemote("Leave_Party", currentPlayerInParty)
	        else
	            if currentPlayerInParty == localPlayer then
	                 print("AutoJoinParty (Untoggled): Current target was self, no leave action needed.")
	            elseif currentPlayerInParty and not currentPlayerInParty.Parent then
	                 print("AutoJoinParty (Untoggled): Current target " .. currentPlayerInParty.Name .. " already left game. No leave action needed.")
	            else
	                 print("AutoJoinParty (Untoggled): No specific party target to leave, or target was already nil.")
	            end
	        end
	        currentPlayerInParty = nil 
	    end
	end)
	
	
end
coroutine.wrap(WVFKMKV_fake_script)()
local function OGZNES_fake_script()  AutoFarm.LocalScript 
	local script = Instance.new('LocalScript', AutoFarm)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local Toggle = false
	
	local button = script.Parent
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	local textBox = button:FindFirstChildWhichIsA("TextBox")
	
	local tpConnection
	local lastVelocity = Vector3.zero
	local lastPosition = nil
	
	 Settings for resolver
	local MAX_HUMAN_SPEED = 100       studs/sec
	local MAX_HUMAN_ACCEL = 200       studs/sec^2
	local SMOOTH_FACTOR = 0.3         lerp smoothing
	
	 Ignore specific IDs
	local ignoreIDs = {
		[10224205063] = true,
		[10246426788] = true
	}
	
	local function findPlayerByName(name)
		if not name or #name == 0 then return nil end
		name = name:lower()
	
		for _, pl in Players:GetPlayers() do
			local n = pl.Name:lower()
			local d = pl.DisplayName:lower()
	
			if n:sub(1, #name) == name or d:sub(1, #name) == name then
				 Skip ignored IDs
				if ignoreIDs[pl.UserId] then
					continue
				end
				return pl
			end
		end
		return nil
	end
	
	local function clampVector3(vec, maxMag)
		if vec.Magnitude > maxMag then
			return vec.Unit * maxMag
		end
		return vec
	end
	
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		if Toggle then
			button.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	
			tpConnection = RunService.Heartbeat:Connect(function(dt)
				if not Toggle then return end
	
				local targetName = textBox and textBox.Text
				local target = findPlayerByName(targetName)
				if not (target and target.Character) then return end
	
				local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
				local myChar = player.Character
				if not (targetHRP and myChar) then return end
	
				local myHRP = myChar:FindFirstChild("HumanoidRootPart")
				if not myHRP then return end
	
				 Get velocity safely
				local velocity = targetHRP.AssemblyLinearVelocity
	
				 Resolver: ignore extreme spikes
				if velocity.Magnitude > MAX_HUMAN_SPEED * 5 then
					 use last known velocity instead
					velocity = lastVelocity
				else
					velocity = clampVector3(velocity, MAX_HUMAN_SPEED)
				end
	
				 distance-based prediction
				local distance = (myHRP.Position - targetHRP.Position).Magnitude
				local leadTime = math.clamp(distance / 60, 0.06, 0.22)
	
				 acceleration estimation
				local accel = (velocity - lastVelocity) / math.max(dt, 1/60)
				accel = clampVector3(accel, MAX_HUMAN_ACCEL)
	
				 predicted position using safe resolver
				local predictedPos
				if lastPosition then
					local avgPos = (lastPosition + targetHRP.Position) / 2
					predictedPos = avgPos + velocity * leadTime + accel * (leadTime ^ 2) * 0.5
				else
					predictedPos = targetHRP.Position + velocity * leadTime + accel * (leadTime ^ 2) * 0.5
				end
	
				lastVelocity = velocity
				lastPosition = targetHRP.Position
	
				local predictedCF = CFrame.new(predictedPos, predictedPos + targetHRP.CFrame.LookVector)
	
				 smooth follow
				myHRP.CFrame = myHRP.CFrame:Lerp(predictedCF, SMOOTH_FACTOR)
			end)
	
		else
			button.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	
			if tpConnection then
				tpConnection:Disconnect()
				tpConnection = nil
			end
			lastVelocity = Vector3.zero
			lastPosition = nil
		end
	end)
	
end
coroutine.wrap(OGZNES_fake_script)()
local function NGLQBLV_fake_script()  HitAura.LocalScript 
	local script = Instance.new('LocalScript', HitAura)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	local auraConn
	
	local ATTACK_RANGE = 5  distance in studs
	
	 Ignore friends button
	local ignoreFriendsButton = button:FindFirstChild("IgnoreFriends")
	local ignoreFriendsActive = false
	
	 List of whitelisted IDs to ignore
	local ignoreIDs = {
		[10192581807] = true,
		[10224205063] = true,
		[10246426788] = true,
		[8493249794]  = true
	}
	
	 =======================
	 UI: Ignore Friends
	 =======================
	if ignoreFriendsButton then
		ignoreFriendsButton.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		ignoreFriendsButton:SetAttribute("IgnoreFriendsActive", false)
	
		ignoreFriendsButton.MouseButton1Click:Connect(function()
			ignoreFriendsActive = not ignoreFriendsActive
			ignoreFriendsButton:SetAttribute("IgnoreFriendsActive", ignoreFriendsActive)
	
			ignoreFriendsButton.BackgroundColor3 =
				ignoreFriendsActive and Color3.fromRGB(74, 74, 74)
				or Color3.fromRGB(15, 15, 15)
		end)
	end
	
	 =======================
	 Utilities
	 =======================
	local function getHRP(char)
		return char and (char:FindFirstChild("HumanoidRootPart") or char:WaitForChild("HumanoidRootPart",1))
	end
	
	local function getHead(char)
		return char and (char:FindFirstChild("Head") or char:WaitForChild("Head",1))
	end
	
	local function hasValidWeaponEquipped(char)
		for _, v in ipairs(char:GetChildren()) do
			if v:IsA("Tool") then
				local name = v.Name:lower()
				if name:find("fist") or name:find("hand") or name:find("unarmed") then
					return false
				end
				return true
			end
		end
		return false
	end
	
	 =======================
	 Target Selection
	 =======================
	local function getNextTarget()
		local myChar = player.Character
		local myHRP = getHRP(myChar)
		if not myHRP then return nil end
	
		local closestPlayer = nil
		local closestDist2 = math.huge
	
		for _, other in ipairs(Players:GetPlayers()) do
			if other ~= player and other.Character then
				if ignoreIDs[other.UserId] then continue end
				if ignoreFriendsActive then
					local ok, isFriend = pcall(function() return player:IsFriendsWith(other.UserId) end)
					if ok and isFriend then continue end
				end
	
				local hum = other.Character:FindFirstChildWhichIsA("Humanoid") or other.Character:WaitForChild("Humanoid",1)
				local hrp = getHRP(other.Character)
				local head = getHead(other.Character)
	
				if hum and hrp and head and hum.Health > 0 then
					local dist = (myHRP.Position - hrp.Position).Magnitude
					if dist <= ATTACK_RANGE and dist < closestDist2 then
						closestDist2 = dist
						closestPlayer = other
					end
				end
			end
		end
	
		return closestPlayer
	end
	
	 =======================
	 Attack Logic
	 =======================
	local function performAttack(target)
		local char = player.Character
		local hrp = getHRP(char)
		if not (char and hrp) then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local core = char:FindFirstChild("Core")
		if not core then return end
		local comm = core:FindFirstChild("Communicate")
		if not comm then return end
	
		local remote = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
		if not remote then return end
		if not (target and target.Character) then return end
	
		local targetChar = target.Character
		local hum = targetChar:FindFirstChildWhichIsA("Humanoid") or targetChar:WaitForChild("Humanoid",1)
		local head = getHead(targetChar)
		if not (hum and head) then return end
		if hum.Health <= 0 then return end
	
		 Fire continuously while left mouse button is held
		while Toggle and UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) do
			 Extra distance check to make sure target is still in range
			local dist = (hrp.Position - getHRP(targetChar).Position).Magnitude
			if dist > ATTACK_RANGE then break end
	
			remote:FireServer("Swing")
			remote:FireServer("Attack", {
				Character = char,
				Point     = hrp.Position,
				Hit       = head,
				Limb      = "Head",
				Result    = nil
			}, false)
			task.wait()
		end
	end
	
	 =======================
	 Aura Loop
	 =======================
	local function startAuraLoop()
		if auraConn then return end
	
		auraConn = coroutine.create(function()
			while Toggle do
				local success, err = pcall(function()
					local target = getNextTarget()
					if target then
						performAttack(target)
					end
				end)
				if not success then
					warn("Aura error: "..tostring(err))
				end
				task.wait()
			end
			auraConn = nil
		end)
	
		coroutine.resume(auraConn)
	end
	
	local function stopAuraLoop()
		if auraConn and coroutine.status(auraConn) ~= "dead" then
			Toggle = false
			auraConn = nil
		end
	end
	
	 =======================
	 Main Toggle Button
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		button.BackgroundColor3 =
			Toggle and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			startAuraLoop()
		else
			stopAuraLoop()
		end
	end)
	
end
coroutine.wrap(NGLQBLV_fake_script)()
local function BGSGI_fake_script()  targethitaura.LocalScript 
	local script = Instance.new('LocalScript', targethitaura)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local Toggle = false
	
	local button = script.Parent
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	local textBox = button:FindFirstChildWhichIsA("TextBox")
	
	local auraConn
	
	 Ignore specific IDs
	local ignoreIDs = {
		[10224205063] = true,
		[10246426788] = true
	}
	
	 =======================
	 Utilities
	 =======================
	local function getHRP(char)
		return char and char:FindFirstChild("HumanoidRootPart")
	end
	
	local function getHead(char)
		return char and char:FindFirstChild("Head")
	end
	
	local function hasValidWeaponEquipped(char)
		for _, v in ipairs(char:GetChildren()) do
			if v:IsA("Tool") then
				local n = v.Name:lower()
				if n:find("fist") or n:find("hand") or n:find("unarmed") then
					return false
				end
				return true
			end
		end
		return false
	end
	
	 =======================
	 Get targets from comma list
	 =======================
	local function getTargetsFromText(text)
		if not text or text == "" then return {} end
	
		local targets = {}
		local used = {}
	
		for name in string.gmatch(text, "([^,]+)") do
			name = name:lower():gsub("^%s+", ""):gsub("%s+$", "")
	
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= player and not ignoreIDs[plr.UserId] and not used[plr] then
					local uname = plr.Name:lower()
					local dname = plr.DisplayName:lower()
	
					if uname:sub(1, #name) == name or dname:sub(1, #name) == name then
						targets[#targets + 1] = plr
						used[plr] = true
						break
					end
				end
			end
		end
	
		return targets
	end
	
	 =======================
	 Attack Logic
	 =======================
	local function performAttack(target)
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		if not hrp then return end
	
		local core = char:FindFirstChild("Core")
		local comm = core and core:FindFirstChild("Communicate")
		if not comm then return end
	
		local remote = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
		if not remote then return end
	
		local tChar = target.Character
		local hum = tChar and tChar:FindFirstChildWhichIsA("Humanoid")
		local head = getHead(tChar)
	
		if not (hum and head) or hum.Health <= 0 then return end
	
		 Fire the attack
		remote:FireServer("Swing")
		remote:FireServer("Attack", {
			Character = char,
			Point     = hrp.Position,
			Hit       = head,
			Limb      = "Head",
			Result    = nil
		}, false)
	end
	
	 =======================
	 Aura Loop
	 =======================
	local function startAuraLoop()
		if auraConn then return end
	
		auraConn = RunService.Heartbeat:Connect(function()
			if not Toggle or not textBox then return end
	
			local targets = getTargetsFromText(textBox.Text)
			for _, target in ipairs(targets) do
				performAttack(target)
			end
		end)
	end
	
	local function stopAuraLoop()
		if auraConn then
			auraConn:Disconnect()
			auraConn = nil
		end
	end
	
	 =======================
	 Toggle Button
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		button.BackgroundColor3 =
			Toggle and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			startAuraLoop()
		else
			stopAuraLoop()
		end
	end)
	
end
coroutine.wrap(BGSGI_fake_script)()
local function WQHNSGK_fake_script()  GlitchTheGame.LocalScript 
	local script = Instance.new('LocalScript', GlitchTheGame)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local Toggle = false
	local loopConn
	local groupId  cached Roblox group id
	
	 =======================
	 Utilities
	 =======================
	local function getRemote()
		local core = player:WaitForChild("Communicate")
		local interface = core:WaitForChild("Interface")
		return interface:WaitForChild("RemoteEvent")
	end
	
	 =======================
	 Roblox Group Detection
	 =======================
	local function getAnyRobloxGroup()
		local success, groups = pcall(function()
			return player:GetGroupsAsync()
		end)
	
		if success and groups then
			for _, group in ipairs(groups) do
				if group.Id then
					return group.Id
				end
			end
		end
	
		return nil
	end
	
	 =======================
	 Group Logic
	 =======================
	local function joinGroup(id)
		getRemote():FireServer("JoinClan", id)
	end
	
	local function leaveGroup(id)
		getRemote():FireServer("LeaveClan", id)
	end
	
	 =======================
	 Loop
	 =======================
	local function groupLoop()
		while Toggle and groupId do
			 join and leave
			joinGroup(groupId)
			task.wait(0.8)
			leaveGroup(groupId)
			task.wait(0.8)
		end
	end
	
	local function enableLoop()
		if loopConn then return end
	
		groupId = getAnyRobloxGroup()
		if not groupId then
			warn("No Roblox group detected.")
			return
		end
	
		 Run the loop in a separate task to avoid blocking RenderStepped
		loopConn = task.spawn(groupLoop)
	end
	
	local function disableLoop()
		Toggle = false
		loopConn = nil
	end
	
	 =======================
	 Button Toggle
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle and Color3.fromRGB(74,74,74) or Color3.fromRGB(15,15,15)
	
		if Toggle then
			enableLoop()
		else
			disableLoop()
		end
	end)
	
end
coroutine.wrap(WQHNSGK_fake_script)()
local function WGBL_fake_script()  HitAura_2.LocalScript 
	local script = Instance.new('LocalScript', HitAura_2)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	local Toggle = false
	local auraConn
	
	local MAX_DISTANCE = 50
	local ATTACK_DELAY = 0
	local lastAttack = 0
	
	 =======================
	 Performance tuning
	 =======================
	local TARGET_SCAN_INTERVAL = 0.01
	local ABUSE_INTERVAL = 0.1
	
	local lastScan = 0
	local lastAbuse = 0
	local cachedTarget = nil
	
	 =======================
	 Ignore Friends
	 =======================
	local ignoreFriendsButton = button:FindFirstChild("IgnoreFriends")
	local ignoreFriendsActive = false
	
	local ignoreIDs = {
		[10220946359] = true,
		[10224205063] = true
	}
	
	 Friend cache (VERY IMPORTANT)
	local friendCache = {}
	
	if ignoreFriendsButton then
		ignoreFriendsButton.BackgroundColor3 = Color3.fromRGB(15,15,15)
	
		ignoreFriendsButton.MouseButton1Click:Connect(function()
			ignoreFriendsActive = not ignoreFriendsActive
			friendCache = {}  reset cache when toggled
	
			ignoreFriendsButton.BackgroundColor3 =
				ignoreFriendsActive and Color3.fromRGB(74,74,74)
				or Color3.fromRGB(15,15,15)
		end)
	end
	
	 =======================
	 Utilities
	 =======================
	local function getHRP(char)
		return char and char:FindFirstChild("HumanoidRootPart")
	end
	
	local function getHead(char)
		return char and char:FindFirstChild("Head")
	end
	
	local function hasValidWeaponEquipped(char)
		for _,v in ipairs(char:GetChildren()) do
			if v:IsA("Tool") then
				local n = v.Name:lower()
				if n:find("fist") or n:find("hand") or n:find("unarmed") then
					return false
				end
				return true
			end
		end
		return false
	end
	
	local function getRemote(char)
		local core = char:FindFirstChild("Core")
		if not core then return end
		local comm = core:FindFirstChild("Communicate")
		if not comm then return end
		return comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
	end
	
	local function isIgnored(plr)
		if ignoreIDs[plr.UserId] then
			return true
		end
	
		if ignoreFriendsActive then
			if friendCache[plr.UserId] == nil then
				local ok, isFriend = pcall(function()
					return player:IsFriendsWith(plr.UserId)
				end)
				friendCache[plr.UserId] = ok and isFriend
			end
			return friendCache[plr.UserId]
		end
	
		return false
	end
	
	 =======================
	 Closest Target
	 =======================
	local function getNextTarget()
		local myChar = player.Character
		local myHRP = getHRP(myChar)
		if not myHRP then return end
	
		local closest, closestDist = nil, math.huge
	
		for _,plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character and not isIgnored(plr) then
				local char = plr.Character
				local hum  = char:FindFirstChildWhichIsA("Humanoid")
				local hrp  = getHRP(char)
				local head = getHead(char)
	
				if hum and hrp and head and hum.Health > 0 then
					local dist = (myHRP.Position - hrp.Position).Magnitude
					if dist <= MAX_DISTANCE and dist < closestDist then
						closestDist = dist
						closest = plr
					end
				end
			end
		end
	
		return closest
	end
	
	 =======================
	 Attack
	 =======================
	local function performAttack(target)
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		local remote = getRemote(char)
		if not (hrp and remote) then return end
	
		local tChar = target.Character
		local hum   = tChar and tChar:FindFirstChildWhichIsA("Humanoid")
		local head  = getHead(tChar)
		if not (hum and head) or hum.Health <= 0 then return end
	
		remote:FireServer("Swing")
		remote:FireServer("Attack", {
			Character = char,
			Point     = hrp.Position,
			Hit       = head,
			Limb      = "Head",
			Result    = nil
		}, false)
	end
	
	 =======================
	 Abuse Everyone (UNCHANGED LOGIC, THROTTLED)
	 =======================
	local function abuseEveryone()
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		local remote = getRemote(char)
		if not (hrp and remote) then return end
	
		for _,plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character and not isIgnored(plr) then
				local char2 = plr.Character
				local hum   = char2:FindFirstChildWhichIsA("Humanoid")
				local head  = getHead(char2)
	
				if hum and head and hum.Health > 0 then
					remote:FireServer("Swing")
					remote:FireServer("Attack", {
						Character = char,
						Point     = hrp.Position,
						Hit       = head,
						Limb      = "Head",
						Result    = nil
					}, false)
				end
			end
		end
	end
	
	 =======================
	 RunService Aura Loop (OPTIMIZED)
	 =======================
	local function startAuraLoop()
		if auraConn then return end
	
		auraConn = RunService.Heartbeat:Connect(function()
			if not Toggle then return end
	
			local now = tick()
	
			 Attack delay
			if now - lastAttack < ATTACK_DELAY then return end
			lastAttack = now
	
			 Throttled target scanning
			if now - lastScan >= TARGET_SCAN_INTERVAL then
				lastScan = now
				cachedTarget = getNextTarget()
			end
	
			 Attack cached target
			if cachedTarget then
				performAttack(cachedTarget)
			end
	
			 Throttled abuse loop
			if now - lastAbuse >= ABUSE_INTERVAL then
				lastAbuse = now
				abuseEveryone()
			end
		end)
	end
	
	local function stopAuraLoop()
		if auraConn then
			auraConn:Disconnect()
			auraConn = nil
		end
	end
	
	 =======================
	 Toggle Button
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		button.BackgroundColor3 =
			Toggle and Color3.fromRGB(74,74,74)
			or Color3.fromRGB(15,15,15)
	
		if Toggle then
			startAuraLoop()
		else
			stopAuraLoop()
		end
	end)
	
end
coroutine.wrap(WGBL_fake_script)()
local function JOBUVD_fake_script()  AutoShove.LocalScript 
	local script = Instance.new('LocalScript', AutoShove)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local button = script.Parent
	local Toggle = false
	
	local character, hrp, remoteEvent 
	local shoveLoop = nil
	local MAX_DISTANCE = 13
	local ATTACK_INTERVAL = 0
	
	 List of whitelisted IDs to ignore
	local ignoreIDs = {
		[10224205063] = true,
		[10220946359] = true
	}
	
	
	 Ignore friends button reference
	local ignoreFriendsButton_HitAura = nil
	local hitAuraButton = script.Parent.Parent:FindFirstChild("AutoShove")
	if hitAuraButton then
		ignoreFriendsButton_HitAura = hitAuraButton:FindFirstChild("IgnoreFriends")
	end
	
	if not ignoreFriendsButton_HitAura then
		warn("AutoShove: Could not find the IgnoreFriends button via HitAura button. Friend ignoring will not work.")
	end
	
	 Update character references
	local function updateCharacterReferences(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart", 5) 
	
		local core = char:FindFirstChild("Core")
		if core then
			local comm = core:FindFirstChild("Communicate")
			if comm then
				remoteEvent = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
				if not remoteEvent then
					warn("AutoShove: Could not find RemoteEvent under Character.Core.Communicate")
				end
			else
				warn("AutoShove: Could not find Communicate folder under Character.Core")
			end
		else
			warn("AutoShove: Could not find Core folder under Character")
		end
	end
	
	 Get closest player ignoring friends and ignored IDs
	local function getClosestPlayer()
		local localChar = player.Character
		if not localChar then return nil end
		local localHrp = localChar:FindFirstChild("HumanoidRootPart")
		if not localHrp then return nil end
	
		local closest, minDist = nil, MAX_DISTANCE
	
		for _, other in ipairs(Players:GetPlayers()) do
			if other ~= player and other.Character then
	
				 Ignore specific IDs
				if ignoreIDs[other.UserId] then
					continue
				end
	
				local otherHrp = other.Character:FindFirstChild("HumanoidRootPart")
				if otherHrp then
					local applyFriendIgnore = false
					if ignoreFriendsButton_HitAura and ignoreFriendsButton_HitAura.Parent then
						if ignoreFriendsButton_HitAura:GetAttribute("IgnoreFriendsActive") then
							applyFriendIgnore = true
						end
					end
	
					if applyFriendIgnore then
						local success, isFriend = pcall(function()
							return player:IsFriendsWith(other.UserId)
						end)
						if success and isFriend then
							continue
						end
					end
	
					local dist = (localHrp.Position - otherHrp.Position).Magnitude
					if dist < minDist then
						minDist = dist
						closest = other
					end
				end
			end
		end
	
		return closest
	end
	
	 Shove the target at their position and stop sounds
	local function shoveTarget(target)
		if not (target and target.Character and remoteEvent) then 
			return 
		end
	
		local targetChar = target.Character
		local targetHrp = targetChar:FindFirstChild("HumanoidRootPart")
		if not targetHrp then
			warn("AutoShove: Target HumanoidRootPart not found for shove.")
			return
		end
	
		local hitPart = targetChar:FindFirstChild("UpperTorso") or targetHrp
	
		 Stop unwanted sounds
		for _, sound in ipairs(targetChar:GetDescendants()) do
			if sound:IsA("Sound") and (sound.Name == "Woosh" or sound.Name == "weave") then
				sound:Stop()
				sound = nil
			end
		end
	
		local args = {
			"Shove",
			{
				Character = targetChar,                 
				Point = targetHrp.Position,             
				Hit = hitPart,                          
				Limb = "",                              
				Result = nil
			}
		}
	
		local success, err = pcall(function()
			remoteEvent:FireServer(unpack(args))
		end)
		if not success then
			warn("AutoShove: Error firing RemoteEvent: " .. tostring(err))
		end
	end
	
	 Start the shove loop
	local function startShoveLoop()
		if shoveLoop then return end 
		shoveLoop = task.spawn(function()
			while Toggle do
				if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
					task.wait() 
					continue
				end
	
				local target = getClosestPlayer()
				if target then
					shoveTarget(target)
				end
	
				task.wait(ATTACK_INTERVAL) 
			end
			shoveLoop = nil 
		end)
	end
	
	 Stop the shove loop
	local function stopShoveLoop()
		if shoveLoop then
			shoveLoop = nil 
		end
	end
	
	 Button toggle
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle and Color3.fromRGB(74, 74, 74) or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			if player.Character then 
				updateCharacterReferences(player.Character) 
				startShoveLoop()
			else
				local charAddedConn
				charAddedConn = player.CharacterAdded:Connect(function(char)
					updateCharacterReferences(char)
					startShoveLoop()
					if charAddedConn then 
						charAddedConn:Disconnect()
						charAddedConn = nil
					end
				end)
			end
		else
			stopShoveLoop()
		end
	end)
	
	player.CharacterAdded:Connect(function(char)
		updateCharacterReferences(char) 
	
		if Toggle then 
			stopShoveLoop() 
			startShoveLoop()
		end
	end)
	
	if player.Character then
		updateCharacterReferences(player.Character)
	end
	
end
coroutine.wrap(JOBUVD_fake_script)()
local function IQAW_fake_script()  AutoSlam.LocalScript 
	local script = Instance.new('LocalScript', AutoSlam)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local Toggle = false
	local character, hrp, remoteEvent
	local MAX_DISTANCE = 99999999
	
	 Optimized timings
	local TARGET_REFRESH_RATE = 0.15
	local ATTACK_RATE = 0.03
	
	local slamConnection = nil
	local scanTimer = 0
	local attackTimer = 0
	local cachedTargets = {}
	local attackIndex = 1
	
	 Ignore list (removed 8493249794)
	local ignoreIDs = {
		[10192581807] = true,
		[10224205063] = true,
		[10246426788] = true
	}
	
	 Ignore friends toggle
	local ignoreFriendsButton_HitAura = nil
	local hitAuraButton = script.Parent.Parent:FindFirstChild("HitAura")
	if hitAuraButton then
		ignoreFriendsButton_HitAura = hitAuraButton:FindFirstChild("IgnoreFriends")
	end
	
	 Follower part
	local followerPart = nil
	local function createFollowerPart()
		if not character or not hrp then return end
	
		if followerPart then
			followerPart:Destroy()
		end
	
		followerPart = Instance.new("Part")
		followerPart.Name = "FollowerPart"
		followerPart.Size = Vector3.new(5, 1, 5)
		followerPart.Anchored = true
		followerPart.CanCollide = false
		followerPart.Transparency = 1
		followerPart.Parent = workspace
	
		RunService.Heartbeat:Connect(function()
			if character and hrp and followerPart then
				followerPart.Position = hrp.Position - Vector3.new(0, hrp.Size.Y/2 + followerPart.Size.Y/2, 0)
			elseif followerPart then
				followerPart:Destroy()
				followerPart = nil
			end
		end)
	end
	
	 Update character references
	local function updateCharacterReferences(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart", 5)
	
		local core = char:FindFirstChild("Core")
		if core then
			local comm = core:FindFirstChild("Communicate")
			if comm then
				remoteEvent = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
			end
		end
	
		createFollowerPart()
	end
	
	 Get all valid targets
	local function getValidTargets()
		if not hrp then return {} end
	
		local targets = {}
	
		for _, other in ipairs(Players:GetPlayers()) do
			if other ~= player and other.Character then
				local userId = other.UserId
				if not ignoreIDs[userId] then
					local ignoreFriend = false
					if ignoreFriendsButton_HitAura
						and ignoreFriendsButton_HitAura:GetAttribute("IgnoreFriendsActive") then
						local success, isFriend = pcall(function()
							return player:IsFriendsWith(userId)
						end)
						if success and isFriend then
							ignoreFriend = true
						end
					end
	
					if not ignoreFriend then
						local otherChar = other.Character
						local partsToCheck = {
							otherChar:FindFirstChild("HumanoidRootPart"),
							otherChar:FindFirstChild("UpperTorso"),
							otherChar:FindFirstChild("LowerTorso"),
							otherChar:FindFirstChild("Head"),
							otherChar:FindFirstChild("LeftUpperArm"),
							otherChar:FindFirstChild("RightUpperArm"),
							otherChar:FindFirstChild("LeftUpperLeg"),
							otherChar:FindFirstChild("RightUpperLeg")
						}
	
						local inRange = false
						for _, part in ipairs(partsToCheck) do
							if part and (hrp.Position - part.Position).Magnitude <= MAX_DISTANCE then
								inRange = true
								break
							end
						end
	
						if inRange then
							table.insert(targets, other)
						end
					end
				end
			end
		end
	
		return targets
	end
	
	 Slam target
	local function slamTarget(target)
		if not (target and target.Character and remoteEvent and hrp) then return end
	
		local targetChar = target.Character
		local targetHrp = targetChar:FindFirstChild("HumanoidRootPart")
		if not targetHrp then return end
	
		local hitPart =
			targetChar:FindFirstChild("UpperTorso")
			or targetChar:FindFirstChild("Torso")
			or targetHrp
	
		remoteEvent:FireServer(
			"Slam",
			{
				Character = character,
				Point = hrp.Position,
				Hit = hitPart,
				Limb = hitPart,
				Result = "Hit"
			}
		)
	end
	
	 Slam loop
	local function startSlamLoop()
		if slamConnection then return end
	
		scanTimer = 0
		attackTimer = 0
		attackIndex = 1
	
		slamConnection = RunService.Heartbeat:Connect(function(dt)
			if not Toggle or not hrp then return end
	
			scanTimer += dt
			if scanTimer >= TARGET_REFRESH_RATE then
				scanTimer = 0
				cachedTargets = getValidTargets()
				attackIndex = 1
			end
	
			attackTimer += dt
			if attackTimer < ATTACK_RATE then return end
			attackTimer = 0
	
			local target = cachedTargets[attackIndex]
			if target then
				slamTarget(target)
				attackIndex += 1
			else
				attackIndex = 1
			end
		end)
	end
	
	local function stopSlamLoop()
		if slamConnection then
			slamConnection:Disconnect()
			slamConnection = nil
		end
	end
	
	 Button toggle
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle
			and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			 Unlock cursor
			UserInputService.MouseBehavior = Enum.MouseBehavior.Default
			UserInputService.MouseIconEnabled = true
	
			if player.Character then
				updateCharacterReferences(player.Character)
				startSlamLoop()
			end
		else
			 Lock cursor back
			UserInputService.MouseBehavior = Enum.MouseBehavior.LockCenter
			UserInputService.MouseIconEnabled = false
	
			stopSlamLoop()
		end
	end)
	
	 Respawn handling
	player.CharacterAdded:Connect(function(char)
		updateCharacterReferences(char)
		if Toggle then
			stopSlamLoop()
			startSlamLoop()
		end
	end)
	
	 Initialize
	if player.Character then
		updateCharacterReferences(player.Character)
	end
	
end
coroutine.wrap(IQAW_fake_script)()
local function TWOUR_fake_script()  Noclip.LocalScript 
	local script = Instance.new('LocalScript', Noclip)

	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local Toggle = false
	local connection
	
	local function setNoclip()
		pcall(function()
			local character = LocalPlayer.Character
			if character then
				for _, part in character:GetDescendants() do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			end
		end)
	end
	
	local function resetCollision()
		pcall(function()
			local character = LocalPlayer.Character
			if character then
				for _, part in character:GetDescendants() do
					if part:IsA("BasePart") then
						part.CanCollide = true
					end
				end
			end
		end)
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		if Toggle then
			script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
			connection = RunService.Stepped:Connect(setNoclip)
		else
			script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
			if connection then
				connection:Disconnect()
				connection = nil
			end
			resetCollision()
		end
	end)
	
	
end
coroutine.wrap(TWOUR_fake_script)()
local function WUOIPS_fake_script()  WalkSpeedChanger.Obfuscated-LocalScript 
	local script = Instance.new('LocalScript', WalkSpeedChanger)

	 Services
	local Players = game:GetService("Players")
	 RunService is not strictly needed if using global task.wait()
	
	 Player and UI
	local LocalPlayer = Players.LocalPlayer
	local button = script.Parent
	local speedTextBox  To be initialized
	
	 Configuration
	local DEFAULT_WALKSPEED = 9  Standard Roblox walkspeed
	local MAX_WALKSPEED = 150  Maximum allowed walkspeed
	local currentSpeedSetting = DEFAULT_WALKSPEED  Holds the speed to be applied when toggled
	
	 State
	local isToggleEnabled = false
	local walkSpeedLoopThread = nil  Stores the coroutine
	
	local function updateHumanoidWalkSpeed(speed)
	    local character = LocalPlayer.Character
	    if character then
	        local humanoid = character:FindFirstChildOfClass("Humanoid")
	        if humanoid then
	            if humanoid.WalkSpeed ~= speed then
	                humanoid.WalkSpeed = speed
	                 print("WalkSpeed set to:", speed)  Optional: for debugging
	            end
	        end
	    end
	end
	
	 This function is the core of the walk speed loop
	local function walkSpeedLoop()
	    print("Walkspeed loop coroutine started. Target speed:", currentSpeedSetting)
	    while isToggleEnabled do
	        local char = LocalPlayer.Character
	        if char and char:FindFirstChildOfClass("Humanoid") then
	            updateHumanoidWalkSpeed(currentSpeedSetting)
	        else
	             Character or humanoid lost, if toggle is still on, we wait. If off, loop will break.
	            if not isToggleEnabled then break end
	        end
	        task.wait()  Yield, allow other scripts to run, and check toggle state frequently
	    end
	     Loop ended because isToggleEnabled became false or an issue like character loss then toggle off
	    print("Walkspeed loop coroutine terminated. Resetting speed to default.")
	    updateHumanoidWalkSpeed(DEFAULT_WALKSPEED)  Reset to default when loop stops
	    walkSpeedLoopThread = nil  Clear the thread reference
	end
	
	local function initializeSpeedTextBox()
	    speedTextBox = button:FindFirstChild("TextBox") 
	
	    if speedTextBox and speedTextBox:IsA("TextBox") then
	        speedTextBox.PlaceholderText = "Speed (1-" .. MAX_WALKSPEED .. ")" 
	        speedTextBox.Text = tostring(currentSpeedSetting) 
	        speedTextBox.Visible = isToggleEnabled
	        speedTextBox.ClearTextOnFocus = false
	
	        speedTextBox.FocusLost:Connect(function(enterPressed)
	            local text = speedTextBox.Text
	            local num = tonumber(text)
	            local newSpeed = currentSpeedSetting 
	
	            if num then
	                newSpeed = math.clamp(num, 1, MAX_WALKSPEED) 
	            else
	                 Not a number, keep currentSpeedSetting. Textbox will be updated to reflect this.
	            end
	            
	            currentSpeedSetting = newSpeed
	            speedTextBox.Text = tostring(currentSpeedSetting) 
	            
	            print("Walkspeed setting updated via FocusLost to:", currentSpeedSetting)
	        end)
	        print("Speed TextBox initialized. Initial speed setting for 'enabled' state:", currentSpeedSetting)
	    else
	        warn("LocalScript: 'TextBox' (TextBox) not found under button '" .. button.Name .. "'. Using default speed " .. currentSpeedSetting .. " when enabled.")
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    isToggleEnabled = not isToggleEnabled
	    button.BackgroundColor3 = isToggleEnabled and Color3.fromRGB(74, 74, 74) or Color3.fromRGB(15, 15, 15)
	
	    if speedTextBox then
	        speedTextBox.Visible = isToggleEnabled
	    end
	
	    if isToggleEnabled then
	        if speedTextBox then
	            local text = speedTextBox.Text
	            local num = tonumber(text)
	            if num then
	                currentSpeedSetting = math.clamp(num, 1, MAX_WALKSPEED)
	            else
	                 If text is not a number, currentSpeedSetting remains as is (from last valid input or default)
	            end
	            speedTextBox.Text = tostring(currentSpeedSetting) 
	        end
	        
	        print("Walkspeed Enabled. Target speed:", currentSpeedSetting)
	        if not walkSpeedLoopThread or coroutine.status(walkSpeedLoopThread) == "dead" then
	            walkSpeedLoopThread = coroutine.create(walkSpeedLoop)
	            coroutine.resume(walkSpeedLoopThread)
	        end
	    else
	        print("Walkspeed Disabled. Immediately resetting speed to default.")
	        updateHumanoidWalkSpeed(DEFAULT_WALKSPEED)  Set to default immediately when toggled off
	         Note: The walkSpeedLoop, if it was active, will also attempt to set the speed
	         to default upon its termination. This is redundant but harmless.
	    end
	end)
	
	LocalPlayer.CharacterAdded:Connect(function(character)
	    print("Character added. Current toggle state:", isToggleEnabled, "Current speed setting:", currentSpeedSetting)
	    local humanoid = character:WaitForChild("Humanoid", 5) 
	    if not humanoid then
	        warn("Humanoid not found for new character after waiting.")
	        return
	    end
	
	    if isToggleEnabled then
	         If toggle is on, ensure the loop is (re)started for the new character
	        if not walkSpeedLoopThread or coroutine.status(walkSpeedLoopThread) == "dead" then
	            print("Character added & toggle is ON. Starting walkspeed loop.")
	            walkSpeedLoopThread = coroutine.create(walkSpeedLoop)
	            coroutine.resume(walkSpeedLoopThread)
	        else
	            print("Character added & toggle is ON. Loop already running/pending.")
	        end
	    end
	     If toggle is OFF, do nothing to the walkspeed. It will use its natural/game-default speed.
	
	    if speedTextBox then
	        speedTextBox.Visible = isToggleEnabled
	    end
	end)
	
	 Initial setup calls
	initializeSpeedTextBox()
	
	 Handle character if it already exists when script loads
	local character = LocalPlayer.Character
	if character then
	    local humanoid = character:FindFirstChildOfClass("Humanoid")
	    if humanoid then
	        if isToggleEnabled then 
	            print("Initial character found & toggle is ON (unusual). Starting loop.")
	            if not walkSpeedLoopThread or coroutine.status(walkSpeedLoopThread) == "dead" then
	                walkSpeedLoopThread = coroutine.create(walkSpeedLoop)
	                coroutine.resume(walkSpeedLoopThread)
	            end
	        end
	         If toggle is OFF, do nothing to the walkspeed.
	    end
	end
	
	 Ensure the UI reflects the initial state (toggle off)
	button.BackgroundColor3 = Color3.fromRGB(15, 15, 15)  Off color
	if speedTextBox then
	    speedTextBox.Visible = false  Off visibility
	    speedTextBox.Text = tostring(currentSpeedSetting) 
	end
	
	
end
coroutine.wrap(WUOIPS_fake_script)()
local function EGZUHH_fake_script()  TextBox_6.LocalScript 
	local script = Instance.new('LocalScript', TextBox_6)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	local textBox = script.Parent
	local defaultHipHeight = humanoid.HipHeight
	
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local hipHeight = tonumber(textBox.Text)
			if hipHeight then
				if hipHeight < 0 then
					humanoid.HipHeight = 2
					textBox.Text = "2"
				else
					humanoid.HipHeight = hipHeight
				end
				defaultHipHeight = humanoid.HipHeight
	
			end
		end
	end)
	
	player.CharacterAdded:Connect(function(newCharacter)
		humanoid = newCharacter:WaitForChild("Humanoid")
		humanoid.HipHeight = defaultHipHeight
	end)
	
	RunService.RenderStepped:Connect(function()
		local hipHeight = tonumber(textBox.Text)
		if hipHeight then
			if hipHeight < 0 then
				humanoid.HipHeight = 2
			else
				humanoid.HipHeight = hipHeight
			end
		else
			humanoid.HipHeight = 2
		end
	end)
	
	
end
coroutine.wrap(EGZUHH_fake_script)()
local function LFUUBR_fake_script()  BringAll.LocalScript 
	local script = Instance.new('LocalScript', BringAll)

	 Services
	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	 State
	local Toggle = false
	local character, hrp = nil, nil
	local slammedPlayers = {}  Tracks who has already been slammed
	local pulling = false  prevents multiple G presses from stacking
	
	 Config
	local MAX_DISTANCE = 100000
	local ignoreIDs = {
		[10192581807] = true,
		[10224205063] = true,
		[10246426788] = true,
		[8493249794]  = true
	}
	
	 Ignore friends toggle reference
	local hitAuraButton = script.Parent.Parent:FindFirstChild("HitAura")
	local ignoreFriendsButton_HitAura = hitAuraButton and hitAuraButton:FindFirstChild("IgnoreFriends")
	
	 Update character references
	local function updateCharacterReferences(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart", 5)
	end
	
	 Get all valid targets
	local function getValidTargets()
		if not hrp then return {} end
		local targets = {}
	
		for _, other in Players:GetPlayers() do
			if other ~= player and other.Character and not slammedPlayers[other.UserId] then
				if ignoreIDs[other.UserId] then continue end
	
				 Ignore friends
				if ignoreFriendsButton_HitAura 
					and ignoreFriendsButton_HitAura:GetAttribute("IgnoreFriendsActive") then
	
					local success, isFriend = pcall(function()
						return player:IsFriendsWith(other.UserId)
					end)
	
					if success and isFriend then
						continue
					end
				end
	
				table.insert(targets, other)
			end
		end
	
		return targets
	end
	
	 Slam bringer effect
	local function slamAll()
		if not hrp or pulling then return end
		pulling = true
		local originalAnchored = hrp.Anchored
		hrp.Anchored = true  anchor yourself
	
		local targets = getValidTargets()
		for _, target in ipairs(targets) do
			local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
			if targetHrp then
				slammedPlayers[target.UserId] = true  mark as slammed
	
				 Pull toward player over 0.3 seconds
				local startPos = targetHrp.Position
				local endPos = hrp.Position + Vector3.new(0,3,0)  slightly above to prevent overlap
				local duration = 0.3
				local elapsed = 0
	
				 Use Heartbeat to tween manually
				local conn
				conn = RunService.Heartbeat:Connect(function(dt)
					if not targetHrp or not hrp then
						conn:Disconnect()
						return
					end
					elapsed += dt
					local alpha = math.clamp(elapsed / duration, 0, 1)
					targetHrp.CFrame = CFrame.new(startPos:Lerp(endPos, alpha))
					if alpha >= 1 then
						conn:Disconnect()
					end
				end)
			end
		end
	
		 Release anchor after all pulled
		delay(0.35, function()
			if hrp then
				hrp.Anchored = originalAnchored
			end
			pulling = false
		end)
	end
	
	 Keybind: G to slam all, only if toggle is on
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if Toggle and input.KeyCode == Enum.KeyCode.G then
			slamAll()
		end
	end)
	
	 Button toggle
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle
			and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	end)
	
	 Respawn handling
	player.CharacterAdded:Connect(function(char)
		updateCharacterReferences(char)
		slammedPlayers = {}  reset on respawn so we can slam again
	end)
	
	 Initialize
	if player.Character then
		updateCharacterReferences(player.Character)
	end
	
end
coroutine.wrap(LFUUBR_fake_script)()
local function BCXYXQ_fake_script()  targethitaura_2.LocalScript 
	local script = Instance.new('LocalScript', targethitaura_2)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local Toggle = false
	
	local button = script.Parent
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	local textBox = button:FindFirstChildWhichIsA("TextBox")
	
	local auraConn
	
	 Ignore specific IDs
	local ignoreIDs = {
		[10224205063] = true,
		[10246426788] = true
	}
	
	 =======================
	 Utilities
	 =======================
	local function getHRP(char)
		return char and char:FindFirstChild("HumanoidRootPart")
	end
	
	local function getHitPart(targetChar)
		 Use UpperTorso > Torso > HumanoidRootPart
		return targetChar:FindFirstChild("UpperTorso")
			or targetChar:FindFirstChild("Torso")
			or getHRP(targetChar)
	end
	
	local function hasValidWeaponEquipped(char)
		for _, v in ipairs(char:GetChildren()) do
			if v:IsA("Tool") then
				local n = v.Name:lower()
				if n:find("fist") or n:find("hand") or n:find("unarmed") then
					return false
				end
				return true
			end
		end
		return false
	end
	
	 =======================
	 Get targets from comma list
	 =======================
	local function getTargetsFromText(text)
		if not text or text == "" then return {} end
	
		local targets = {}
		local used = {}
	
		for name in string.gmatch(text, "([^,]+)") do
			name = name:lower():gsub("^%s+", ""):gsub("%s+$", "")
	
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= player and not ignoreIDs[plr.UserId] and not used[plr] then
					local uname = plr.Name:lower()
					local dname = plr.DisplayName:lower()
	
					if uname:sub(1, #name) == name or dname:sub(1, #name) == name then
						targets[#targets + 1] = plr
						used[plr] = true
						break
					end
				end
			end
		end
	
		return targets
	end
	
	 =======================
	 Slam Attack Logic
	 =======================
	local function performSlam(target)
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		if not hrp then return end
	
		local core = char:FindFirstChild("Core")
		local comm = core and core:FindFirstChild("Communicate")
		if not comm then return end
	
		local remote = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
		if not remote then return end
	
		local tChar = target.Character
		local hum = tChar and tChar:FindFirstChildWhichIsA("Humanoid")
		local hitPart = tChar and getHitPart(tChar)
	
		if not (hum and hitPart) or hum.Health <= 0 then return end
	
		remote:FireServer("Slam", {
			Character = char,
			Point     = hrp.Position,
			Hit       = hitPart,
			Limb      = hitPart,
			Result    = "Hit"
		})
	end
	
	 =======================
	 Follower Part
	 =======================
	local followerPart = nil
	local function createFollowerPart()
		local char = player.Character
		local hrp = getHRP(char)
		if not char or not hrp then return end
	
		if followerPart then
			followerPart:Destroy()
		end
	
		followerPart = Instance.new("Part")
		followerPart.Name = "FollowerPart"
		followerPart.Size = Vector3.new(5, 1, 5)
		followerPart.Anchored = true
		followerPart.CanCollide = false
		followerPart.Transparency = 1
		followerPart.Parent = workspace
	
		RunService.Heartbeat:Connect(function()
			if char and hrp and followerPart then
				followerPart.Position = hrp.Position - Vector3.new(0, hrp.Size.Y/2 + followerPart.Size.Y/2, 0)
			elseif followerPart then
				followerPart:Destroy()
				followerPart = nil
			end
		end)
	end
	
	 =======================
	 Aura Loop
	 =======================
	local function startAuraLoop()
		if auraConn then return end
	
		auraConn = RunService.Heartbeat:Connect(function()
			if not Toggle then return end
			if not textBox then return end
	
			local targets = getTargetsFromText(textBox.Text)
			for _, target in ipairs(targets) do
				performSlam(target)
			end
		end)
	end
	
	local function stopAuraLoop()
		if auraConn then
			auraConn:Disconnect()
			auraConn = nil
		end
	end
	
	 =======================
	 Toggle Button
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		button.BackgroundColor3 =
			Toggle and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			createFollowerPart()
			startAuraLoop()
		else
			stopAuraLoop()
		end
	end)
	
	 =======================
	 Respawn Handling
	 =======================
	player.CharacterAdded:Connect(function(char)
		if Toggle then
			createFollowerPart()
		end
	end)
	
	 Initialize
	if player.Character then
		createFollowerPart()
	end
	
end
coroutine.wrap(BCXYXQ_fake_script)()
local function CUAY_fake_script()  AutoShove_2.LocalScript 
	local script = Instance.new('LocalScript', AutoShove_2)

	 =======================
	 Services
	 =======================
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local ReplicatedFirst = game:GetService("ReplicatedFirst")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	 =======================
	 State
	 =======================
	local Toggle = false
	local character, hrp, remoteEvent
	local shoveLoop = nil
	
	 =======================
	 Ignore IDs
	 =======================
	local ignoreIDs = {
		[10224205063] = true,
		[10246426788] = true
	}
	
	 =======================
	 Ignore Friends Button
	 =======================
	local ignoreFriendsButton_HitAura = nil
	local hitAuraButton = script.Parent.Parent:FindFirstChild("AutoShove")
	if hitAuraButton then
		ignoreFriendsButton_HitAura = hitAuraButton:FindFirstChild("IgnoreFriends")
	end
	
	if not ignoreFriendsButton_HitAura then
		warn("AutoShove: IgnoreFriends button not found.")
	end
	
	 =======================
	 Character Setup
	 =======================
	local function updateCharacterReferences(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart", 5)
	
		local core = char:FindFirstChild("Core")
		if core then
			local comm = core:FindFirstChild("Communicate")
			if comm then
				remoteEvent = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
			end
		end
	end
	
	 =======================
	 Ignore Logic
	 =======================
	local function shouldIgnore(target)
		if ignoreIDs[target.UserId] then
			return true
		end
	
		if ignoreFriendsButton_HitAura and ignoreFriendsButton_HitAura:GetAttribute("IgnoreFriendsActive") then
			local ok, isFriend = pcall(function()
				return player:IsFriendsWith(target.UserId)
			end)
			if ok and isFriend then
				return true
			end
		end
	
		return false
	end
	
	 =======================
	 Shove Target
	 =======================
	local function shoveTarget(target)
		if not (target and target.Character and remoteEvent and hrp) then return end
	
		local targetChar = target.Character
		local targetHrp = targetChar:FindFirstChild("HumanoidRootPart")
		if not targetHrp then return end
	
		local hitPart = targetChar:FindFirstChild("UpperTorso") or targetHrp
	
		 Kill shove sounds
		for _, s in ipairs(targetChar:GetDescendants()) do
			if s:IsA("Sound") and (s.Name == "Woosh" or s.Name == "weave") then
				s:Stop()
			end
		end
	
		local args = {
			"Shove",
			{
				Character = character,
				Point = hrp.Position,
				Hit = hitPart,
				Limb = "",
				Result = nil
			}
		}
	
		pcall(function()
			remoteEvent:FireServer(unpack(args))
		end)
	end
	
	 =======================
	 Shove Everyone
	 =======================
	local function shoveEveryone()
		for _, target in ipairs(Players:GetPlayers()) do
			if target ~= player and target.Character and not shouldIgnore(target) then
				shoveTarget(target)
			end
		end
	end
	
	 =======================
	 Loop Control
	 =======================
	local function startShoveLoop()
		if shoveLoop then return end
		shoveLoop = task.spawn(function()
			while Toggle do
				if hrp then
					shoveEveryone()
				end
				task.wait(0.1)
			end
			shoveLoop = nil
		end)
	end
	
	local function stopShoveLoop()
		shoveLoop = nil
	end
	
	 =======================
	 Button Toggle
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle and Color3.fromRGB(74, 74, 74) or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			if player.Character then
				updateCharacterReferences(player.Character)
				startShoveLoop()
			end
			startCameraShakerRemoval()
		else
			stopShoveLoop()
		end
	end)
	
	 =======================
	 Respawn Handling
	 =======================
	player.CharacterAdded:Connect(function(char)
		updateCharacterReferences(char)
		if Toggle then
			stopShoveLoop()
			startShoveLoop()
			startCameraShakerRemoval()
		end
	end)
	
	 Initial load
	if player.Character then
		updateCharacterReferences(player.Character)
	end
	
	 =======================
	 CameraShaker Removal
	 =======================
	local function removeCameraShaker(container)
		for _, obj in ipairs(container:GetDescendants()) do
			if obj.Name == "CameraShaker" then
				obj:Destroy()
			end
		end
	end
	
	function startCameraShakerRemoval()
		 initial cleanup
		local ps = player:WaitForChild("PlayerScripts")
		removeCameraShaker(ps)
		removeCameraShaker(ReplicatedFirst)
	
		 watch for re-added CameraShaker
		ps.DescendantAdded:Connect(function(obj)
			if obj.Name == "CameraShaker" then
				obj:Destroy()
			end
		end)
		ReplicatedFirst.DescendantAdded:Connect(function(obj)
			if obj.Name == "CameraShaker" then
				obj:Destroy()
			end
		end)
	end
	
end
coroutine.wrap(CUAY_fake_script)()
local function JOTCMB_fake_script()  AutoSlam_2.LocalScript 
	local script = Instance.new('LocalScript', AutoSlam_2)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local Toggle = false
	local character, hrp, remoteEvent
	local MAX_DISTANCE = 99999999
	
	 Optimized timings
	local TARGET_REFRESH_RATE = 0.15
	local ATTACK_RATE = 0.03
	
	local slamConnection = nil
	local scanTimer = 0
	local attackTimer = 0
	local cachedTargets = {}
	local attackIndex = 1
	
	 Ignore list (removed 8493249794)
	local ignoreIDs = {
		[10192581807] = true,
		[10224205063] = true,
		[10246426788] = true
	}
	
	 Ignore friends toggle
	local ignoreFriendsButton_HitAura = nil
	local hitAuraButton = script.Parent.Parent:FindFirstChild("HitAura")
	if hitAuraButton then
		ignoreFriendsButton_HitAura = hitAuraButton:FindFirstChild("IgnoreFriends")
	end
	
	 Follower part
	local followerPart = nil
	local function createFollowerPart()
		if not character or not hrp then return end
	
		if followerPart then
			followerPart:Destroy()
		end
	
		followerPart = Instance.new("Part")
		followerPart.Name = "FollowerPart"
		followerPart.Size = Vector3.new(5, 1, 5)
		followerPart.Anchored = true
		followerPart.CanCollide = false
		followerPart.Transparency = 1
		followerPart.Parent = workspace
	
		RunService.Heartbeat:Connect(function()
			if character and hrp and followerPart then
				followerPart.Position = hrp.Position - Vector3.new(0, hrp.Size.Y/2 + followerPart.Size.Y/2, 0)
			elseif followerPart then
				followerPart:Destroy()
				followerPart = nil
			end
		end)
	end
	
	 Update character references
	local function updateCharacterReferences(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart", 5)
	
		local core = char:FindFirstChild("Core")
		if core then
			local comm = core:FindFirstChild("Communicate")
			if comm then
				remoteEvent = comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
			end
		end
	
		createFollowerPart()
	end
	
	 Get all valid targets
	local function getValidTargets()
		if not hrp then return {} end
	
		local targets = {}
	
		for _, other in ipairs(Players:GetPlayers()) do
			if other ~= player and other.Character then
				local userId = other.UserId
				if not ignoreIDs[userId] then
					local ignoreFriend = false
					if ignoreFriendsButton_HitAura
						and ignoreFriendsButton_HitAura:GetAttribute("IgnoreFriendsActive") then
						local success, isFriend = pcall(function()
							return player:IsFriendsWith(userId)
						end)
						if success and isFriend then
							ignoreFriend = true
						end
					end
	
					if not ignoreFriend then
						local otherChar = other.Character
						local partsToCheck = {
							otherChar:FindFirstChild("HumanoidRootPart"),
							otherChar:FindFirstChild("UpperTorso"),
							otherChar:FindFirstChild("LowerTorso"),
							otherChar:FindFirstChild("Head"),
							otherChar:FindFirstChild("LeftUpperArm"),
							otherChar:FindFirstChild("RightUpperArm"),
							otherChar:FindFirstChild("LeftUpperLeg"),
							otherChar:FindFirstChild("RightUpperLeg")
						}
	
						local inRange = false
						for _, part in ipairs(partsToCheck) do
							if part and (hrp.Position - part.Position).Magnitude <= MAX_DISTANCE then
								inRange = true
								break
							end
						end
	
						if inRange then
							table.insert(targets, other)
						end
					end
				end
			end
		end
	
		return targets
	end
	
	 Slam target
	local function slamTarget(target)
		if not (target and target.Character and remoteEvent and hrp) then return end
	
		local targetChar = target.Character
		local targetHrp = targetChar:FindFirstChild("HumanoidRootPart")
		if not targetHrp then return end
	
		local hitPart =
			targetChar:FindFirstChild("UpperTorso")
			or targetChar:FindFirstChild("Torso")
			or targetHrp
	
		remoteEvent:FireServer(
			"Slam",
			{
				Character = character,
				Point = hrp.Position,
				Hit = hitPart,
				Limb = hitPart,
				Result = "Hit"
			}
		)
	end
	
	 Slam loop
	local function startSlamLoop()
		if slamConnection then return end
	
		scanTimer = 0
		attackTimer = 0
		attackIndex = 1
	
		slamConnection = RunService.Heartbeat:Connect(function(dt)
			if not Toggle or not hrp then return end
	
			scanTimer += dt
			if scanTimer >= TARGET_REFRESH_RATE then
				scanTimer = 0
				cachedTargets = getValidTargets()
				attackIndex = 1
			end
	
			attackTimer += dt
			if attackTimer < ATTACK_RATE then return end
			attackTimer = 0
	
			local target = cachedTargets[attackIndex]
			if target then
				slamTarget(target)
				attackIndex += 1
			else
				attackIndex = 1
			end
		end)
	end
	
	local function stopSlamLoop()
		if slamConnection then
			slamConnection:Disconnect()
			slamConnection = nil
		end
	end
	
	 Button toggle
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
		button.BackgroundColor3 = Toggle
			and Color3.fromRGB(74, 74, 74)
			or Color3.fromRGB(15, 15, 15)
	
		if Toggle then
			if player.Character then
				updateCharacterReferences(player.Character)
				startSlamLoop()
			end
		else
			stopSlamLoop()
		end
	end)
	
	 Respawn handling
	player.CharacterAdded:Connect(function(char)
		updateCharacterReferences(char)
		if Toggle then
			stopSlamLoop()
			startSlamLoop()
		end
	end)
	
	 Initialize
	if player.Character then
		updateCharacterReferences(player.Character)
	end
	
end
coroutine.wrap(JOTCMB_fake_script)()
local function VCZBD_fake_script()  HitAura_3.LocalScript 
	local script = Instance.new('LocalScript', HitAura_3)

	local Players    = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	local Toggle = false
	local auraConn
	
	local MAX_DISTANCE = 99999999999999
	local ATTACK_DELAY = 0
	local lastAttack = 0
	
	 =======================
	 Performance tuning
	 =======================
	local TARGET_SCAN_INTERVAL = 0.01
	local ABUSE_INTERVAL = 0.05
	
	local lastScan = 0
	local lastAbuse = 0
	local cachedTarget = nil
	
	 =======================
	 Ignore Friends
	 =======================
	local ignoreFriendsButton = button:FindFirstChild("IgnoreFriends")
	local ignoreFriendsActive = false
	
	local ignoreIDs = {
		[10220946359] = true,
		[10224205063] = true
	}
	
	 Friend cache (VERY IMPORTANT)
	local friendCache = {}
	
	if ignoreFriendsButton then
		ignoreFriendsButton.BackgroundColor3 = Color3.fromRGB(15,15,15)
	
		ignoreFriendsButton.MouseButton1Click:Connect(function()
			ignoreFriendsActive = not ignoreFriendsActive
			friendCache = {}  reset cache when toggled
	
			ignoreFriendsButton.BackgroundColor3 =
				ignoreFriendsActive and Color3.fromRGB(74,74,74)
				or Color3.fromRGB(15,15,15)
		end)
	end
	
	 =======================
	 Utilities
	 =======================
	local function getHRP(char)
		return char and char:FindFirstChild("HumanoidRootPart")
	end
	
	local function getHead(char)
		return char and char:FindFirstChild("Head")
	end
	
	local function hasValidWeaponEquipped(char)
		for _,v in ipairs(char:GetChildren()) do
			if v:IsA("Tool") then
				local n = v.Name:lower()
				if n:find("fist") or n:find("hand") or n:find("unarmed") then
					return false
				end
				return true
			end
		end
		return false
	end
	
	local function getRemote(char)
		local core = char:FindFirstChild("Core")
		if not core then return end
		local comm = core:FindFirstChild("Communicate")
		if not comm then return end
		return comm:FindFirstChild("blank") or comm:FindFirstChildWhichIsA("RemoteEvent")
	end
	
	local function isIgnored(plr)
		if ignoreIDs[plr.UserId] then
			return true
		end
	
		if ignoreFriendsActive then
			if friendCache[plr.UserId] == nil then
				local ok, isFriend = pcall(function()
					return player:IsFriendsWith(plr.UserId)
				end)
				friendCache[plr.UserId] = ok and isFriend
			end
			return friendCache[plr.UserId]
		end
	
		return false
	end
	
	 =======================
	 Closest Target
	 =======================
	local function getNextTarget()
		local myChar = player.Character
		local myHRP = getHRP(myChar)
		if not myHRP then return end
	
		local closest, closestDist = nil, math.huge
	
		for _,plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character and not isIgnored(plr) then
				local char = plr.Character
				local hum  = char:FindFirstChildWhichIsA("Humanoid")
				local hrp  = getHRP(char)
				local head = getHead(char)
	
				if hum and hrp and head and hum.Health > 0 then
					local dist = (myHRP.Position - hrp.Position).Magnitude
					if dist <= MAX_DISTANCE and dist < closestDist then
						closestDist = dist
						closest = plr
					end
				end
			end
		end
	
		return closest
	end
	
	 =======================
	 Attack
	 =======================
	local function performAttack(target)
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		local remote = getRemote(char)
		if not (hrp and remote) then return end
	
		local tChar = target.Character
		local hum   = tChar and tChar:FindFirstChildWhichIsA("Humanoid")
		local head  = getHead(tChar)
		if not (hum and head) or hum.Health <= 0 then return end
	
		remote:FireServer("Swing")
		remote:FireServer("Attack", {
			Character = char,
			Point     = hrp.Position,
			Hit       = head,
			Limb      = "Head",
			Result    = nil
		}, false)
	end
	
	 =======================
	 Abuse Everyone (UNCHANGED LOGIC, THROTTLED)
	 =======================
	local function abuseEveryone()
		local char = player.Character
		if not char then return end
		if not hasValidWeaponEquipped(char) then return end
	
		local hrp = getHRP(char)
		local remote = getRemote(char)
		if not (hrp and remote) then return end
	
		for _,plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character and not isIgnored(plr) then
				local char2 = plr.Character
				local hum   = char2:FindFirstChildWhichIsA("Humanoid")
				local head  = getHead(char2)
	
				if hum and head and hum.Health > 0 then
					remote:FireServer("Swing")
					remote:FireServer("Attack", {
						Character = char,
						Point     = hrp.Position,
						Hit       = head,
						Limb      = "Head",
						Result    = nil
					}, false)
				end
			end
		end
	end
	
	 =======================
	 RunService Aura Loop (OPTIMIZED)
	 =======================
	local function startAuraLoop()
		if auraConn then return end
	
		auraConn = RunService.Heartbeat:Connect(function()
			if not Toggle then return end
	
			local now = tick()
	
			 Attack delay
			if now - lastAttack < ATTACK_DELAY then return end
			lastAttack = now
	
			 Throttled target scanning
			if now - lastScan >= TARGET_SCAN_INTERVAL then
				lastScan = now
				cachedTarget = getNextTarget()
			end
	
			 Attack cached target
			if cachedTarget then
				performAttack(cachedTarget)
			end
	
			 Throttled abuse loop
			if now - lastAbuse >= ABUSE_INTERVAL then
				lastAbuse = now
				abuseEveryone()
			end
		end)
	end
	
	local function stopAuraLoop()
		if auraConn then
			auraConn:Disconnect()
			auraConn = nil
		end
	end
	
	 =======================
	 Toggle Button
	 =======================
	button.MouseButton1Click:Connect(function()
		Toggle = not Toggle
	
		button.BackgroundColor3 =
			Toggle and Color3.fromRGB(74,74,74)
			or Color3.fromRGB(15,15,15)
	
		if Toggle then
			startAuraLoop()
		else
			stopAuraLoop()
		end
	end)
	
end
coroutine.wrap(VCZBD_fake_script)()
