	local script = Instance.new('LocalScript', TouchFling)

	
	
	local RunService = game:GetService("RunService") local Players = game:GetService("Players") local LocalPlayer = Players.LocalPlayer local Toggle = false local connection  local function setVelocity() pcall(function() local RootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") if RootPart then local oldVelocity = RootPart.Velocity local oldALVelocity = RootPart.AssemblyLinearVelocity  RootPart.Velocity = Vector3.new(1,1,1) * -(2^16) RootPart.AssemblyLinearVelocity = Vector3.new(1,1,1) * -(2^16)  RunService.RenderStepped:Wait()  RootPart.Velocity = oldVelocity RootPart.AssemblyLinearVelocity = oldALVelocity end end) end  script.Parent.MouseButton1Click:Connect(function() Toggle = not Toggle if Toggle then script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74) connection = RunService.Heartbeat:Connect(setVelocity) else script.Parent.BackgroundColor3 = Color3.fromRGB(15, 15, 15) if connection then connection:Disconnect() connection = nil end end end)   
end
